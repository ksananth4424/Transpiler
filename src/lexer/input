set int small
set float small

func total_area ( [float] h, [int] b ; float)  # comment
<
  float sum=0;
  loop (int i=0 ; i < size[h] and i < size[b] 0; i=i+1)  
  < sum = sum+h[i]*b[i]; >
  return sum;
>

[float] h, b;
a<-(0.5); b<-(3);
a<-(1.5); b<-(2);
a<-(2.5); b<-(1);
